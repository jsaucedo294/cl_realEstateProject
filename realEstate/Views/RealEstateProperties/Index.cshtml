@model List<RealEstatePropertyShared.Models.RealEstateProperty>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <div class="container">
        <div class="row">
            

            @foreach (var property in Model)
            {
            <div class="col-md-4">
                <div class="property-card">
                   

                    <h2>@property.Zpid</h2>
                </div>
            </div>
            }

        </div>
    </div>

